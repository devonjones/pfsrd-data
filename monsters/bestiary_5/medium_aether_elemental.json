        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Telekinetic Invisibility",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex",
                "text": "An aether elemental\u2019s body bends light and dampens sound waves. This works as the <a href=\"KineticistTalentsDisplay.aspx?ItemName=Telekinetic%20Invisibility\" style=\"text-decoration:underline\">kineticist utility wild talent telekinetic invisibility</a>, except that it is constant and doesn\u2019t end when the elemental attacks. As this ability is inherent, it is not subject to effects such as <a href=\"SpellDisplay.aspx?ItemName=invisibility%20purge\" style=\"text-decoration:underline\"><i>invisibility purge</i></a>. "
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Telekinetic Throw",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex",
                "text": "An aether elemental can fling a creature or object (with a maximum range of 480 feet). A creature receives a Fortitude save to avoid being thrown (the save DC is Constitution-based). The telekinetic throw deals the listed damage to both the thrown creature or object and the target. The aether elemental can throw a creature or object that weighs at most 50 pounds per Hit Die the elemental possesses. "
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Telekinetic Deflection",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su",
                "text": "An aether elemental gains a deflection bonus to its AC equal to its Constitution modifier. "
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Telekinetic Maneuver",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su",
                "text": "An aether elemental can attempt a ranged combat maneuver check, as per <a href=\"SpellDisplay.aspx?ItemName=telekinesis\" style=\"text-decoration:underline\"><i>telekinesis</i></a>, with a CMB equal to its Hit Dice + its Dexterity modifier."
            }
        ]
