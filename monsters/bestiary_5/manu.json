        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Aura of Unity",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su",
                "text": "All allies that belong to the manu\u2019s creature bond work toward a unified goal while within 30 feet of the manu. As an immediate action, the manu can attempt an attack roll, saving throw, or skill check to replace the roll just made by an ally benefitting from the manu\u2019s aura of unity."
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Creature Bond",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex",
                "text": "Each manu is associated with a single race or kind of creature, such as humans or basilisks (not humanoids or magical beasts); this bond never changes. The manu is treated as both a manasaputra and as the race, creature type, and subtypes of its bonded creature for the purposes of effects that target a specific race or kind of creature."
            }
        ]
