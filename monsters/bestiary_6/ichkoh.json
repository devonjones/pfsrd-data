        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Fall Apart",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su",
                "text": "As a standard action once every 1d4 rounds, an ichkoh can cause all creatures within 30 feet of it to feel as if their bodies were falling apart unless they succeed at a DC 19 Will save. Those affected by this ability take 1d3 points of Constitution damage. In addition, they take a \u20132 penalty on attack rolls, damage rolls, and skill checks for 1d6 rounds. This is a mind-affecting effect. The save DC is Charisma-based. "
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "name": "Look of Fear",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su",
                "text": "A creature affected by an ichkoh\u2019s gaze is shaken for 1d4 rounds."
            }
