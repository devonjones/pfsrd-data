        "tactics": [
            {
                "type": "stat_block_section",
                "subtype": "tactic",
                "name": "Before Combat",
                "text": "The bard posts political screeds in alleys, imbuing them with sepia snake sigils to trap those reading them. If anticipating combat, the bard drinks his potion of <i>eagle\u2019s splendor</i>."
            },
            {
                "type": "stat_block_section",
                "subtype": "tactic",
                "name": "During Combat",
                "text": "The bard starts by casting <i>haste </i>and <i>mirror image</i>. He then shoots at opposing spellcasters or deafens them with sound burst."
            },
            {
                "type": "stat_block_section",
                "subtype": "tactic",
                "name": "Base Statistics",
                "text": "Without <i>eagle\u2019s splendor</i>, the bard\u2019s statistics are <b>Bard Spells Known </b>reduce spell DCs by 2; <b>Cha </b>17; <b>Skills </b>Bluff +10, Perform (dance) +14."
            }
